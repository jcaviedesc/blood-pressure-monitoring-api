#!/bin/bash
echo -e "DATABASE_CONNECTION_URI=${DATABASE_CONNECTION_UR}" > .env
echo -e "DATABASE_NAME=${DATABASE_NAME}" >> .env
echo -e "GOOGLE_APPLICATION_CREDENTIALS_PATH=${GOOGLE_APPLICATION_CREDENTIALS_PATH}" >> .env

echo  "${GOOGLE_APPLICATION_CREDENTIALS}" > "${GOOGLE_APPLICATION_CREDENTIALS_PATH}"

cat ${GOOGLE_APPLICATION_CREDENTIALS_PATH}